		"ls_Asql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_Asql'"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
