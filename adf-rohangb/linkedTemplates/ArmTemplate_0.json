			"type": "string",
			"defaultValue": "rohangbserver.database.windows.net"
		},
		"ls_Asql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "trainingdb"
		},
		"ls_Asql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin2025"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsrohangb.dfs.core.windows.net/"
		}
